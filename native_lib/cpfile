

######## runtimes-win-x86-native
rm native_lib_win_x86.zip
mkdir workspace-x86
cd workspace-x86
cp /d/Love2DCS_Dev/LOVE2D_CS_DLL_git/megasource/build/love/Release/*.dll        ./
#mv  love.dll    liblove-11.2
zip -r native_lib_win_x86.zip ./*
mv native_lib_win_x86.zip ../
cd ..
rm -rf workspace-x86


######## runtimes-win-x64-native
rm native_lib_win_x64.zip
mkdir workspace-x64
cd workspace-x64
cp /d/Love2DCS_Dev/LOVE2D_CS_DLL_git_64/megasource/build/love/Release/*.dll        ./
#mv  love.dll    liblove-11.2
zip -r native_lib_win_x64.zip ./*
mv native_lib_win_x64.zip ../
cd ..
rm -rf workspace-x64